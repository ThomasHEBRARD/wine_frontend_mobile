{
    "presets": ["module:metro-react-native-babel-preset"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "public": "./public",
                    "src": "./src",
                    // "components": "./src/components",
                    "pages": "./src/pages"
                },
                "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
        ]
    ]
}
